{
  "name": "stop-angular-overrides",
  "description": "Prevents silent angular module / controller / filter / service / directive overrides",
  "version": "1.0.0",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "contributors": [
    "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
    "Tilman Potthof <tilmanpotthof@gmail.com>",
    "Tristan Benschop <tbenschop@verklizan.com>"
  ],
  "dependencies": {},
  "devDependencies": {
    "benv": "3.3.0",
    "coveralls": "2.13.1",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-jshint": "1.1.0",
    "grunt-deps-ok": "0.9.0",
    "grunt-npm2bower-sync": "0.9.1",
    "gt": "0.10.0",
    "matchdep": "1.0.1",
    "pre-git": "3.15.0",
    "q": "1.5.0",
    "time-grunt": "1.4.0"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "> 0.8"
  },
  "homepage": "https://github.com/TristanBenschop/stop-angular-overrides",
  "keywords": [
    "angular",
    "stop",
    "debugging",
    "testing",
    "override"
  ],
  "license": "MIT",
  "main": "stop-angular-overrides.js",
  "pre-commit": [
    "npm run build",
    "npm test",
    "npm version"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/TristanBenschop/stop-angular-overrides.git"
  },
  "scripts": {
    "build": "grunt",
    "test": "gt --output test/*test.js",
    "coveralls": "cat cover/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "commit": "commit-wizard"
  },
  "config": {
    "pre-git": {
      "commit-msg": [],
      "pre-commit": [],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
